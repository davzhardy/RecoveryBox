{
  "expo": {
    "name": "RecoveryBox",
    "slug": "RecoveryBox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mazethernandez.recoverybox",
      "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "android": {
    "package": "com.mazethernandez.recoverybox",
    "versionCode": 1,
    "splash": {
      "resizeMode": "cover",
      "mdpi":  "./assets/androidsplash.png"
    }
  }

}
